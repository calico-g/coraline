<div class="header">

	<div class="container-fluid">

		<div class="logout row">
			<div class="col-sm-12">
				<% if user_signed_in? %>
				  <li>
				  <%= link_to('Logout', destroy_user_session_path, :method => :delete) %>        
				  </li>
				<% else %>
				  <li>
				  <%= link_to('log in', new_user_session_path)  %>
				  <%= link_to('sign up', new_user_registration_path) %>
				  </li>
				<% end %>
			</div>
		</div>
		
		<div class="headline row">
			<div class="col-sm-12">
				<%= link_to(root_url) do %>
				  <h1>coraline</h1>
				<% end %>
			</div>
		</div>
		
		<hr>

	</div>

</div>